{{!
    @template local_sitesetting/customise

    Note: collapse/show functionality as well as check/uncheck functionality is 
          developed using javascript (custom.js) file. Any change in HTML DOM 
          structure can break these functionalities, so change cautiously.

    @author: 2021 Akashy Pingale (akashy.p@paradisosolution.com)
    @paradiso

}}
<table class="table card-box table-people">
  <thead>
    <tr>
    {{#head}}
      <th class="header c0" scope="col">{{{checkbox}}}</th>
      <th class="header c1" scope="col"><a href="{{{firstnameurl}}}" class="sorting">{{#str}}fullname, local_people{{/str}}{{#phsortingup}}<i class="men men-icon-phsortingup pl-1"></i>{{/phsortingup}}{{#phsortingdown}}<i class="men men-icon-phsortingdown pl-1"></i>{{/phsortingdown}}{{#phsorting}}<i class="men men-icon-phsorting pl-1"></i>{{/phsorting}}</a></th>
      <th class="header c2" scope="col">{{#str}}email, local_people{{/str}}</th>
      <th class="header c3" scope="col">{{#str}}dept, local_people{{/str}}</th>
      <th class="header c7" scope="col">{{#str}}courseenrolled, local_people{{/str}}</th>
      <th class="header c8" scope="col">{{#str}}coursecompleted, local_people{{/str}}</th>
      {{#tenant}}
      <th class="header c9" scope="col">{{#str}}multitenant, local_people{{/str}}</th>
      {{/tenant}}
      <th class="header c10" scope="col"><a href="{{{lastaccessurl}}}" class="sorting">{{#str}}lastaccess, local_people{{/str}}{{#phsortingup}}<i class="men men-icon-phsortingup pl-1"></i>{{/phsortingup}}{{#phsortingdown}}<i class="men men-icon-phsortingdown pl-1"></i>{{/phsortingdown}}{{#phsorting}}<i class="men men-icon-phsorting pl-1"></i>{{/phsorting}}</a></th>
      <th class="header c11 lastcol" scope="col">{{#str}}status, local_people{{/str}}</th>

      {{/head}}
      <!--th class="header c12 lastcol" scope="col"></th-->
    </tr>
  </thead>
  <tbody>
  {{#istablerows}}
    <tr>
      {{#issuspenduser}}
      <td><span class="usersuspended">{{{checkbox}}}</span></td>
      <td><span class="usersuspended">{{{fullname}}}</span></td>
      <td><span class="usersuspended">{{{email}}}</span></td>
      <td><span class="usersuspended">{{{dept}}}</span></td>
      <td><span class="usersuspended">{{{enroll}}}</span></td>
      <td><span class="usersuspended">{{{complete}}}</span></td>
      {{#istenant}}<td><span class="usersuspended">{{{tenant_comp}}}</span></td>{{/istenant}}
      <td><span class="usersuspended">{{{skill}}}</span></td>
      <td><span class="usersuspended">{{{position}}}</span></td>
      <td><span class="usersuspended">{{{level}}}</span></td>
      <td><span class="usersuspended">{{{strlastaccess}}}</span></td>
      <td class="lastcol"><span class="usersuspended">{{#suspended}}<i class="fa fa-minus-circle icon_suspend"></i>{{/suspended}}{{^suspended}}<i class="fa fa-check-circle icon_active"></i>{{/suspended}}</span>
      {{#istenant}}
      <span class="usersuspended"><div class="dropdown options-table">
      <a href="#" class="btn dropdown-toggle" data-toggle="dropdown">
          <i class="fa fa-ellipsis-v fa-fw"></i></a><div class="dropdown-menu">{{#loginasbtn}}<a href="{{{urlloginas}}}" title="" class="dropdown-item">{{#str}}loginas,local_people{{/str}}</a>{{/loginasbtn}}{{#hasloginasbtn}}<a href="{{{hasurlloginas}}}" title="{{#str}}loginas,local_people{{/str}}" class="dropdown-item">{{#str}}loginas,local_people{{/str}}</a>{{/hasloginasbtn}}{{#iseditbtn}}<a href="{{{urledit}}}" title="{{#str}}edit{{/str}}" class="dropdown-item">{{#str}}edit,local_people{{/str}}</a>{{/iseditbtn}}{{#issuspendbtn}}<a href="{{{urlsus}}}" class="dropdown-item" title="">{{{suspendstring}}}</a> {{/issuspendbtn}}{{#hasconfirmbtn}}<a href="{{{hasconfirmurl}}}" title="{{#str}}confirm{{/str}}">{{#str}}confirm{{/str}}</a>{{/hasconfirmbtn}}{{#isconfirmbtn}}
            <a href="#" title="{{#str}}info_confirm_user,local_people{{/str}}"><i alt="{{#str}}info_confirm_user,local_people{{/str}}" class="iconsmall men men-icon-helpdesk"></i></a>{{/isconfirmbtn}}{{#isconfirmbtn}}
            <a href="{{{confirmurl}}}" title="{{#str}}send_email_confirmation_again,local_people{{/str}}"><i alt="{{#str}}send_email_confirmation_again,local_people{{/str}}" class="iconsmall wid wid-icon-email"></i></a>{{/isconfirmbtn}}{{#isconfirmbuttn}}<a href="{{{confirmurlbtn}}}" title="{{#str}}confirm{{/str}}">{{#str}}confirm{{/str}}</a>{{/isconfirmbuttn}}{{#isconfirmbutton}}
            <a class="dimmed_text"><i class="iconsmall fa fa-check-circle"></i></a>{{/isconfirmbutton}}</div></div>
      </span></td>{{/istenant}}
      
      {{/issuspenduser}}
      {{^issuspenduser}}
      <td>{{{checkbox}}}</td>
      <td>{{#isupdateuser}}<a href="{{{isupdateuserurl}}}" class="td-click">{{{fullname}}}</a>{{/isupdateuser}}{{^isupdateuser}}{{{fullname}}}{{/isupdateuser}}</td>
      <td>{{{email}}}</td>
      <td>{{{dept}}}</td>
      <td>{{{enroll}}}</td>
      <td>{{{complete}}}</td>
      {{#istenant}}<td>{{{tenant_comp}}}</td>{{/istenant}}
      <td>{{{strlastaccess}}}</td>
      <td class="lastcol">{{#suspended}}<i class="fa fa-minus-circle icon_suspend"></i>{{/suspended}}{{^suspended}}<i class="fa fa-check-circle icon_active"></i>{{/suspended}}{{#istenant}}
      
      <div class="dropdown options-table">
      <a href="#" class="btn dropdown-toggle" data-toggle="dropdown">
          <i class="fa fa-ellipsis-v fa-fw "></i></a><div class="dropdown-menu">{{#loginasbtn}}<a href="{{{urlloginas}}}" title="" class="dropdown-item">{{#str}}loginas,local_people{{/str}}</a>{{/loginasbtn}}{{#hasloginasbtn}}<a href="{{{hasurlloginas}}}" title="{{#str}}loginas,local_people{{/str}}" class="dropdown-item">{{#str}}loginas,local_people{{/str}}</a>{{/hasloginasbtn}}{{#iseditbtn}}<a href="{{{urledit}}}" title="{{#str}}edit{{/str}}" class="dropdown-item">{{#str}}edit,local_people{{/str}}</a>{{/iseditbtn}}{{#issuspendbtn}}<a href="{{{urlsus}}}" class="dropdown-item" title="">{{{suspendstring}}}</a> {{/issuspendbtn}}{{#hasconfirmbtn}}<a href="{{{hasconfirmurl}}}" title="{{#str}}confirm{{/str}}">{{#str}}confirm{{/str}}</a>{{/hasconfirmbtn}}{{#isconfirmbtn}}
            <a href="#" title="{{#str}}info_confirm_user,local_people{{/str}}"><i alt="{{#str}}info_confirm_user,local_people{{/str}}" class="iconsmall men men-icon-helpdesk"></i></a>{{/isconfirmbtn}}{{#isconfirmbtn}}
            <a href="{{{confirmurl}}}" title="{{#str}}send_email_confirmation_again,local_people{{/str}}"><i alt="{{#str}}send_email_confirmation_again,local_people{{/str}}" class="iconsmall wid wid-icon-email"></i></a>{{/isconfirmbtn}}{{#isconfirmbuttn}}<a href="{{{confirmurlbtn}}}" title="{{#str}}confirm{{/str}}">{{#str}}confirm{{/str}}</a>{{/isconfirmbuttn}}{{#isconfirmbutton}}
            <a class="dimmed_text"><i class="iconsmall fa fa-check-circle"></i></a>{{/isconfirmbutton}}</div></div>
      </td>{{/istenant}}
      
      {{/issuspenduser}}
      
    </tr>
    {{/istablerows}}
  </tbody>
</table>
