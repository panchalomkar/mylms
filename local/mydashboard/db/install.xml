                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="categoryid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="assignpoint" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="Record inserted time"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="The unique id for SC"/>
            </KEYS>
        </TABLE>

         <TABLE NAME="assign_smeleader" COMMENT="Assing category point info">
