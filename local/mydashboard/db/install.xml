                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="smeleader_id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="roleid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="categoryid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="Record inserted time"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="The unique id for SC"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="sme_leader_feedback" COMMENT="Level info">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="categoryid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="smeleader_id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="feedback_userid" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="feedback" TYPE="text" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="rate" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" />
                <FIELD NAME="timecreated" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="Record inserted time"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="The unique id for SC"/>
            </KEYS>
        </TABLE>
        
    </TABLES>
</XMLDB>