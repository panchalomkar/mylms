{{!
    @template local_iomadcustompage/local/dynamictabs/details

    Template for Page details tab

    Example context (json):
    {
        "pagedetails": "The page content",
        "pageid": 10
        "contextid": 10
    }
}}
<div class="container local_iomadcustompage-details-container">
    {{{ pagedetails }}}
</div>

{{#js}}
    require(['local_iomadcustompage/details'], function(details) {
      details.init({{pageid}},{{contextid}});
    });
{{/js}}
